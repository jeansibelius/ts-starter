{
  "name": "ts-starter",
  "version": "1.0.0",
  "description": "TypeScript starter repository with eslint",
  "main": "index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Rafael Linnankoski",
  "license": "MIT",
  "lint-staged": {
    "**/*": "yarn prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^9.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "3.3.2",
    "tsc": "^2.0.4",
    "tsx": "^4.16.0",
    "typescript": "^5.3.2"
  },
  "packageManager": "yarn@4.3.1"
}
